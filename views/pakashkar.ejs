                                            <option value="SC">SC</option>
                                            <option value="ST">ST</option>
                                            <option value="EWS">EWS</option>
                                            <option value="OBC">OBC</option>
                                        </select>
                                    </div>
                                    <div class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">जाति </label>
                                        <input type="text" class="form-control inputName"  placeholder="ENTER SUMMARY">
                                    </div>
                                    <div class=" mb-5">
