    <script>
      function fun(val){
        let तरफ=document.getElementById("तरफ")
        let मुख्त्यार=document.getElementById("मुख्त्यार")
        let दस्तावेज=document.getElementById("दस्तावेज")
        let ग्रन्थ=document.getElementById("ग्रन्थ")
        let पृष्ट=document.getElementById("पृष्ट")
        let इ=document.getElementById("इ")
        let पंजीयन=document.getElementById("पंजीयन")
        let संरक्षक=document.getElementById("संरक्षक")
        let न्यायलय=document.getElementById("न्यायलय")
        let आदेश=document.getElementById("आदेश")
        let दिनांक=document.getElementById("दिनांक")
        if(val=="मुख्त्यार"){
            तरफ.style.display="block";
            मुख्त्यार.style.display="block";
            दस्तावेज.style.display="block";
            ग्रन्थ.style.display="block";
            पृष्ट.style.display="block";
            इ.style.display="block";
            पंजीयन.style.display="block";
            दिनांक.style.display="block";
            आदेश.style.display="none";
            संरक्षक.style.display="none";
            न्यायलय.style.display="none";
            
        }else if(val=="संरक्षक"){
            तरफ.style.display="block";
            मुख्त्यार.style.display="none";
            दस्तावेज.style.display="none";
            ग्रन्थ.style.display="none";
            पृष्ट.style.display="none";
            इ.style.display="none";
            पंजीयन.style.display="none";
            दिनांक.style.display="none";
            आदेश.style.display="block";
            संरक्षक.style.display="block";
            न्यायलय.style.display="block";
        }else{
            तरफ.style.display="none";
            मुख्त्यार.style.display="none";
            दस्तावेज.style.display="none";
            ग्रन्थ.style.display="none";
            पृष्ट.style.display="none";
            इ.style.display="none";
            पंजीयन.style.display="none";
            दिनांक.style.display="none";
            आदेश.style.display="none";
            संरक्षक.style.display="none";
            न्यायलय.style.display="none";
        }
      }
     
    </script>
