            <div class="col-sm-9">
                <form action="" method="post">
                    <div class="form">
                        <div style="display: flex;justify-content: space-between;">
                            <p>पक्षकार का विवरण </p>
                            <button  style="background-color: #313044;border-radius: 23px;padding: 10px 20px;color: white;">ADD</button>
                        </div>
                        <div class="normal">
                            <div class="row">
                                <div class="col-sm-6">
                    
                                    <div class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">पक्षकार का प्रकार</label>
                                        <select class="form-select inputName" onchange="fun(this.value)"  aria-label="Default select example">
                                            <option selected hidden>Choose</option>
                                            <option value="मुख्त्यार">मुख्त्यार आम </option>
                                            <option value="संरक्षक">संरक्षक</option>
                                            <% for(let i=0;i<parray1.length;i++){ %>
                                                <option  value="<%=parray1[i].name%>" ><%=parray1[i].name%></option>
                                           <%} %>
                                        </select>
                                    </div>
                                    <div id="तरफ" class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">तरफ से </label>
                                        <input type="text" class="form-control inputName"  placeholder="ENTER SUMMARY">
                                    </div>
                                    <div class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">पहचान पत्र</label>
                                        <select class="form-select inputName"  aria-label="Default select example">
                                            <option hidden selected>Choose</option>
                                            <% for(let i=0;i<parray.length;i++){ %>
                                                <option  value="<%=parray[i].name%>" ><%=parray[i].name%></option>
                                           <%} %>
                                        </select>
                                    </div>
                                    <div class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">क्रमांक </label>
                                        <input type="number" class="form-control inputName"  placeholder="ENTER SUMMARY">
                                    </div>
                                    <div class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">पैन कार्ड</label>
                                        <input type="text" class="form-control inputName"  placeholder="ENTER SUMMARY">
                                    </div>
                                    <div class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">Designation</label>
                                        <select class="form-select inputName"  aria-label="Default select example">
                                            <option value="Mr.">Mr.</option>
                                            <option value="Mrs.">Mrs.</option>
                                            <option value="Ms.">Ms.</option>
                                        </select>
                                    </div>
                                    <div class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">मोबाइल न- </label>
                                        <input type="number" class="form-control inputName"  placeholder="ENTER SUMMARY">
                                    </div>
                                    <div class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">संरक्षक चयन करे (हिंदी में)</label>
                                        <select class="form-select inputName"  aria-label="Default select example">
                                            <option value="Father">Father</option>
                                            <option value="Mother">Mother</option>
                                            <option value="Husband">Husband</option>
                                            <option value="Garudian">Garudian</option>
                                        </select>
                                    </div>
                                    <div id="मुख्त्यार" class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">मुख्त्यार आम का विवरण भरे </label>
                                        <input type="text" class="form-control inputName"  placeholder="ENTER SUMMARY">
                                    </div>
                                    <div id="पंजीयन" class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">पंजीयन कार्यालय का नाम </label>
                                        <input type="text" class="form-control inputName"  placeholder="ENTER SUMMARY">
                                    </div>
                                    <div id="संरक्षक" class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">संरक्षक होने संबंथी विवरण</label>
                                        <input type="text" class="form-control inputName"  placeholder="ENTER SUMMARY">
                                    </div>
                                 
                                    <div id="दिनांक" class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">दिनांक  </label>
                                        <input type="date" class="form-control inputName"  placeholder="ENTER SUMMARY">
                                    </div>
                                </div>

                                <div class="col-sm-6">
                                    <div class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">लिंग </label>
                                        <select class="form-select inputName"  aria-label="Default select example">
                                            <option value="Male">Male</option>
                                            <option value="Female">Female</option>
                                            <option value="Other">Other</option>
                                        </select>
                                    </div>
                                    <div class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">वर्ग </label>
                                        <select class="form-select inputName" aria-label="Default select example">
