    <section class="bottom">
        <div class="container">
            <div class="row">
                <div class="col-md-12 mb-4 mt-4">
                    <input type="text" id="searchInput" class="form-control" placeholder="Search...">
                </div>
                <% properties.forEach(function(property) { %>
                    <% for(var j=0; j < property.vigypaan.length; j++){ %>
                        <div class="col-md-4 mb-4 property-card">
                            <div class="card h-100 d-flex flex-column">
                                <div class="card-body">
                                    <h5 class="card-title">Property Details</h5>
                                    <p class="card-text">Owner name: <%= property.name %></p>
                                    <p class="card-text">Type: <%= property.vigypaan[j].type %></p>
                                    <p class="card-text">Village Name: <%= property.vigypaan[j].villageName %></p>
                                    <p class="card-text">Land Type: <%= property.vigypaan[j].landType %></p>
                                    <p class="card-text">Price: <%= property.vigypaan[j].price %></p>
                                    <% if (property.vigypaan[j].landType === 'Plot') { %>
                                        <p class="card-text">Area: <%= property.vigypaan[j].area %> varg ft.</p>
                                    <% } else if (property.vigypaan[j].landType === 'House') { %>
                                        <p class="card-text">BHK: <%= property.vigypaan[j].bhk %></p>
                                        <p class="card-text">House Type: <%= property.vigypaan[j].houseType %></p>
                                    <% } %>
                                    <p class="card-text">Address: <%= property.vigypaan[j].address %></p>
                                    <p class="card-text">Description: <%= property.vigypaan[j].description %></p>
                                    <p class="card-text">Contact number: <%= property.phone %></p>
                                </div>
                                <div class="mt-auto"></div>
                            </div>
                        </div>
                    <% } %>
                <% }); %>
            </div>
        </div>
    </section>
