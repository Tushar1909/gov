    <section class="bottom">
        <div class="row">
            <div class="col-sm-3 define">
                <ul>
                    <a style="text-decoration: none;" href="/new">
                        <li>दस्तावेज का विवरण
                        </li>
                    </a>
                    <br>
                    <li> संपत्ति का विवरण भरे
                    </li>
                    <br>
                    <li>संपत्ति का बाजार मूल्य जाँच करे
                    </li>
                    <br>
                    <li>संपत्ति का चौहद्दी एवं अन्य विवरण भरे
                    </li>
                    <br>
                    <a href="/Mumbai/Tushar/stamp" style="text-decoration: none;">
                        <li> स्टाम्प शुल्क और पंजियन शुल्क गणना
                        </li>
                    </a>
                    <br>
                    <a href="/Mumbai/pakashkar" style="text-decoration: none;">
                        <li>पक्षकार का विवरण
                        </li>
                    </a>
                    <br>
                    <li> दस्तावेज के आतंरिक स्वरुप चुने या स्वयं तैयार करे
                    </li>
                    <br>
                    <li>गवाह का विवरण
                    </li>
                    <br>
                    <li>दस्तावेज उत्पन्न कर प्रिंट प्रीव्यू देखे एडिट करे/सबमिट करे/डाउनलोड करे/मेल-whatsapp करे
                    </li>
                </ul>
            </div>
            <div class="col-sm-9">
                <form action="/new" method="post">
                    <div class="form">

                        <!-- दस्तावेज का विवरण -->
                        <div>
                            <p id="hea">दस्तावेज का विवरण </p>
                        </div>
                        <div class="mb-5">
                            <label for="inputRemarks" class="col-sm-2 col-form-label">विलेख चुने</label>
                            <% var uni=unit1; %>
                                <select required class="form-select inputName"
                                    onchange="fun(<%= JSON.stringify(uni) %>,this.value)" name="doc"
                                    aria-label="Default select example">
                                    <option value="विलेख चुने" hidden selected style="color: grey;">विलेख चुने</option>
                                    <% for(let i=0;i<unit1.length;i++){ %>
                                        <option value="<%=unit1[i].name%>">
                                            <%=unit1[i].name%>
                                        </option>
                                        <%} %>
                                </select>
                        </div>
                        <div class="row">
                            <div class="col-sm-6">
                                <div class="mb-5">
                                    <label for="formGroupExampleInput" class="form-label">विलेख का विवरण </label>
                                    <input type="text" name="docdes" class="form-control inputName" readonly id="docdes"
                                        placeholder="">
                                </div>
                                <div class="mb-5">
                                    <label for="formGroupExampleInput" class="form-label">दस्तावेज का प्रकार</label>
                                    <select class="form-select inputName" name="sub" id="sub"
                                        onchange="fun1(<%= JSON.stringify(uni) %>,this.value)"
                                        aria-label="Default select example">
                                    </select>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div id="parsk" class="mb-5">
                                    <label for="formGroupExampleInput" class="form-label">पक्षकारो के मध्य
                                        सम्बन्ध</label>
                                    <select class="form-select inputName" name="parskh" id="formGroupExampleInput"
                                        aria-label="Default select example">
                                        <% for(let i=0;i<parskh.length;i++){ %>
                                            <option value="<%=parskh[i].name%>">
                                                <%=parskh[i].name%>
                                            </option>
                                            <%} %>
                                    </select>

                                </div>
                                <div class="mb-5">
                                    <label for="formGroupExampleInput" class="form-label">लिखत का विवरण </label>
                                    <input type="text" class="form-control inputName" id="subdes" name="subdes" readonly
                                        placeholder="ENTER SUMMARY">
                                </div>

                            </div>
                        </div>

                        <!-- Sampati hak -->
                        <div id="sampati">
                            <div>
                                <p>Sampati hak </p>
                            </div>
                            <div class="mb-5">
                                <label for="formGroupExampleInput" class="form-label">Sampati hak</label>
                                <select class="form-select inputName" id="formGroupExampleInput"
                                    onchange="fun2(<%= JSON.stringify(samp) %>,this.value)"
                                    aria-label="Default select example">
                                    <option value="" hidden selected>choose</option>
                                    <% for(let i=0;i<samp.length;i++){ %>
                                        <option value="<%=samp[i].name%>">
                                            <%=samp[i].name%>
                                        </option>
                                        <%} %>
                                </select>

                            </div>
                            <div id="SSampati" class="row">
                                <div class="col-sm-6">
                                    <div class="mb-5">
                                        <label for="s1" class="form-label">के आदेश क्रमांक </label>
                                        <input type="text" required class="form-control inputName" id="s1"
                                            placeholder="ENTER SUMMARY">
                                    </div>
                                    <div class="mb-5">
                                        <label for="s2" class="form-label">कार्यालय </label>
                                        <input type="text" required class="form-control inputName" id="s2"
                                            placeholder="ENTER SUMMARY">

                                    </div>

                                </div>
                                <div class="col-sm-6">

                                    <div class="mb-5">
                                        <label for="s3" class="form-label">दिनांक </label>
                                        <input type="date" id="s3" class="form-control inputName">
                                    </div>

                                    <div class="mb-5">
                                        <label for="s4" class="form-label">द्वारा अनुमति आदेश जारी </label>
                                        <input type="file" id="s4" class="form-control inputName">
                                    </div>

                                </div>
                            </div>
                        </div>

                        <!-- Aadiwasi ki bhumi ke sambandh me  -->
                        <div id="Aadiwasi">
                            <div>
                                <p>Aadiwasi ki bhumi ke sambandh me </p>
                            </div>
                            <div class="mb-5">
                                <label for="formGroupExampleInput" class="form-label">Aadiwasi ki bhumi ke sambandh
                                    me</label>
                                <select class="form-select inputName" required id="formGroupExampleInput"
                                    onchange="fun3(this.value)" aria-label="Default select example">
                                    <option value="" hidden selected>choose</option>
                                    <option value="Yes">Yes</option>
                                    <option value="No">No</option>
                                </select>

                            </div>

                            <div id="AAadiwasi" class="row">
                                <div class="col-sm-6">
                                    <div class="mb-5">
                                        <label for="a1" class="form-label">के आदेश क्रमांक </label>
                                        <input type="text" required class="form-control inputName" id="a1"
                                            placeholder="ENTER SUMMARY">
                                    </div>
                                    <div class="mb-5">
                                        <label for="a2" class="form-label">कार्यालय </label>
                                        <input type="text" required class="form-control inputName" id="a2"
                                            placeholder="ENTER SUMMARY">

                                    </div>

                                </div>
                                <div class="col-sm-6">

                                    <div class="mb-5">
                                        <label for="a3" class="form-label">दिनांक </label>
                                        <input type="date" id="a3" class="form-control inputName">
                                    </div>

                                    <div class="mb-5">
                                        <label for="a4" class="form-label">द्वारा अनुमति आदेश जारी </label>
                                        <input type="file" id="a4" class="form-control inputName">
                                    </div>

                                </div>
                            </div>
                        </div>


                        <!-- PRATIFAL -->
                        <div id="PRATIFAL">
                            <div class="container" id="partialHolder">
                                <div style="display: flex;justify-content: space-between;">
                                    <p id="pratifalCounter">PRATIFAL 1</p>
                                    <!-- <button onclick="fun5()" style="background-color: #313044;border-radius: 23px;padding: 10px 20px;color: white;">ADD</button> -->
                                </div>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="mb-5">
                                            <label for="formGroupExampleInput" class="form-label">PRATIFAL KA PRAKAR
                                            </label>
                                            <select class="form-select inputName" onchange="fun4(this.value)" required
                                                id="formGroupExampleInput" aria-label="Default select example">
                                                <option value="" hidden selected>Choose</option>
                                                <option value="Cash">Cash</option>
                                                <option value="Cheque">Cheque</option>
                                                <option value="RTGS">RTGS</option>
                                                <option value="NFTS">NFTS</option>
                                                <option value="UPI">UPI</option>
                                            </select>
                                        </div>
                                        <div class="mb-5">
                                            <label for="formGroupExampleInput" class="form-label">JAARI DINANK </label>
                                            <input type="date" required class="form-control inputName"
                                                id="formGroupExampleInput">

                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div id="checkr" class="mb-5">
                                            <label for="checkr1" class="form-label">CHECK/RTGS NO/NFTS No/UPI Id </label>
                                            <input type="text" required id="checkr1" class="form-control inputName">
                                        </div>
                                        <div class="mb-5">
                                            <label for="formGroupExampleInput" class="form-label">RASHI</label>
                                            <input type="number" required id="formGroupExampleInput"
                                                class="form-control inputName">
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div id="bank" class="mb-5">
                                            <label for="bank1" class="form-label">BANK KA NAAM </label>
                                            <input type="text" required id="bank1" class="form-control inputName">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <a class="btn btn-success btn-sm" onclick="addPratifal()">Add Pratifal</a>
                        </div>
                        <div class="mb-5">
                            <label for="formGroupExampleInput" class="form-label">SAMPATI VIVRAN KI AVASHAKTA </label>
                            <select class="form-select inputName" required id="formGroupExampleInput"
                                aria-label="Default select example">
                                <option value="One">One</option>
                                <option value="Two">Two</option>
                                <option value="Three">Three</option>
                            </select>
                        </div>

                        <div class="submit">
                            <button type="submit" class="m-5 submitbtn">Save and next</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </section>
    <script>
        var d;
        function fun(arr, val) {
            d = val;
            let dynamicPratifal = document.querySelectorAll(".pratifal-section");
            let doc = document.getElementById("docdes");
            let Aadiwasi = document.getElementById("Aadiwasi");
            let PRATIFAL = document.getElementById("PRATIFAL");
            let sampati = document.getElementById("sampati");
            let sub = document.getElementById("sub");
            let subdes = document.getElementById("subdes");
            let parsk = document.getElementById("parsk");
            let a1 = document.getElementById("a1");
            let a2 = document.getElementById("a2");
            let s1 = document.getElementById("s1");
            let s2 = document.getElementById("s2");
            let checkr1 = document.getElementById("checkr1");
            let bank1 = document.getElementById("bank1");
            let text = `<option  value="दस्तावेज का प्रकार"  hidden selected>दस्तावेज का प्रकार</option> `
            // Reset pratifal input fields of vilekh change
            if(PRATIFAL){
                PRATIFAL.querySelectorAll("input").forEach((element) => {
                    element.value = "";
                });
            }
            // remove all dynamically created pratifal when Vilekh is changed
            if(dynamicPratifal){
                dynamicPratifal.forEach((element) => {
                    element.remove();
                });
            }


            for (let i = 0; i < arr.length; i++) {
                if (arr[i].name == val) {
                    doc.value = arr[i].description
                    for (let k = 0; k < arr[i].sub.length; k++) {
                        text = text + `<option  value="${arr[i].sub[k].name}">${arr[i].sub[k].name}</option> `
                    }
                    sub.innerHTML = text;
                    subdes.value = " "
                    if (arr[i].par == "Yes") {
                        parsk.style.display = "block";
                    }
                    else {
                        parsk.style.display = "none";
                    }
                    if (arr[i].adi == "Yes") {
                        a1.value = ""
                        a2.value = ""
                        Aadiwasi.style.display = "block";
                    }
                    else {
                        a1.value = " "
                        a2.value = " "
                        Aadiwasi.style.display = "none";
                    }
                    if (arr[i].PRATIFAL == "Yes") {
                        checkr1.value = ""
                        bank1.value = ""
                        PRATIFAL.style.display = "block";
                    }
                    else {
                        checkr1.value = " "
                        bank1.value = " "
                        PRATIFAL.style.display = "none";
                    }
                    if (arr[i].Sampati == "Yes") {
                        s1.value = ""
                        s2.value = ""
                        sampati.style.display = "block";
                    }
                    else {
                        s1.value = " "
                        s2.value = " "
                        sampati.style.display = "none";
                    }
                }
            }
        }

        function fun1(arr, val) {
            console.log(d)
            let doc = document.getElementById("doc");
            let sub = document.getElementById("sub");
            let subdes = document.getElementById("subdes");
            for (let i = 0; i < arr.length; i++) {
                if (arr[i].name == d) {
                    for (let k = 0; k < arr[i].sub.length; k++) {
                        if (arr[i].sub[k].name == val) {
                            subdes.value = arr[i].sub[k].description
                        }
                    }
                }
            }
        }

        function fun2(arr, val) {
            let doc = document.getElementById("SSampati");
            let s1 = document.getElementById("s1");
            let s2 = document.getElementById("s2");
            for (let i = 0; i < arr.length; i++) {
                if (arr[i].name == val) {
                    if (arr[i].other == "Yes") {
                        s1.value = ""
                        s2.value = ""
                        doc.style.display = "flex";
                    }
                    else {
                        s1.value = " "
                        s2.value = " "
                        doc.style.display = "none";

                    }
                }
            }
        }

        function fun3(val) {
            let doc = document.getElementById("AAadiwasi");
            let a1 = document.getElementById("a1");
            let a2 = document.getElementById("a2");

            if (val == "Yes") {
                a1.value = ""
                a2.value = ""
                doc.style.display = "flex";
            } else {
                a1.value = " "
                a2.value = " "
                doc.style.display = "none";

            }
        }

        function fun4(val) {
            let doc = document.getElementById("SSampati");
            let checkr = document.getElementById("checkr");
            let bank = document.getElementById("bank");
            let checkr1 = document.getElementById("checkr1");
            let bank1 = document.getElementById("bank1");
            if (val == "Cash") {
                checkr.style.display = "none"
                bank.style.display = "none"
                checkr1.value = " "
                bank1.value = " "
            } else {
                checkr.style.display = "block"
                bank.style.display = "block"
                checkr1.value = ""
                bank1.value = ""
            }
        }


    </script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm"
        crossorigin="anonymous"></script>
    <script src="/js/new.js"></script>
</body>

</html>


