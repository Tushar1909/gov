    <section class="bottom">
        <div class="row">
            <div class="col-sm-3 define">
                <ul>
                    <li>दस्तावेज का विवरण
                    </li>
                    <br>
                    <li> संपत्ति का विवरण भरे
                    </li>
                    <br>
                    <li>संपत्ति का बाजार मूल्य जाँच करे
                    </li>
                    <br>
                    <li>संपत्ति का चौहद्दी एवं अन्य विवरण भरे
                    </li>
                    <br>
                    <li> स्टाम्प शुल्क और पंजियन शुल्क गणना
                    </li>
                    <br>
                    <li>पक्षकार का विवरण
                    </li>
                    <br>
                    <li> दस्तावेज के आतंरिक स्वरुप चुने या स्वयं तैयार करे
                    </li>
                    <br>
                    <li>गवाह का विवरण
                    </li>
                    <br>
                    <li>दस्तावेज उत्पन्न कर प्रिंट प्रीव्यू देखे एडिट करे/सबमिट करे/डाउनलोड करे/मेल-whatsapp करे
                    </li>
                </ul>
            </div>
            <div class="col-sm-9">
                <form action="/new" method="post">
                    <div class="form">
    
                        <!-- दस्तावेज का विवरण -->
                        <div>
                            <p id="hea">दस्तावेज का विवरण </p>
                        </div>
                        <div class="mb-5">
                            <label for="inputRemarks" class="col-sm-2 col-form-label">विलेख चुने</label>
                            <% var uni=unit1; %>
                            <select required class="form-select inputName" onchange="fun(<%= JSON.stringify(uni) %>,this.value)" name="doc"  aria-label="Default select example">
                                <option value="विलेख चुने" hidden  selected style="color: grey;">विलेख चुने</option>
                              <% for(let i=0;i<unit1.length;i++){ %>
                                    <option  value="<%=unit1[i].name%>" ><%=unit1[i].name%></option>
                               <%} %>
                            </select>                   
                         </div>
                        <div class="row">
                            <div class="col-sm-6">
                                <div class="mb-5">
                                    <label for="formGroupExampleInput" class="form-label">विलेख  का विवरण </label>
                                    <input type="text" name="docdes"  class="form-control inputName" readonly id="docdes" placeholder="">
                                </div>
                                <div class="mb-5">
                                    <label for="formGroupExampleInput" class="form-label">दस्तावेज का प्रकार</label>
                                    <select class="form-select inputName" name="sub" id="sub" onchange="fun1(<%= JSON.stringify(uni) %>,this.value)" aria-label="Default select example">
                                    </select>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div id="parsk" class="mb-5">
                                    <label for="formGroupExampleInput" class="form-label">पक्षकारो के मध्य सम्बन्ध</label>
                                    <select class="form-select inputName" name="parskh" id="formGroupExampleInput"  aria-label="Default select example">
                                        <option value="पक्षकारो चुने1">पक्षकारो चुने 1</option>
                                        <option value="पक्षकारो चुने2">पक्षकारो चुने 2</option>
                                    </select>
    
                                </div>
                                <div class="mb-5">
                                    <label for="formGroupExampleInput" class="form-label">लिखत का विवरण </label>
                                    <input type="text" class="form-control inputName" id="subdes" name="subdes" readonly  placeholder="ENTER SUMMARY">
                                </div>
    
                            </div>
                        </div>
    
                        <!-- Sampati hak -->
    
                        <div>
                            <p>Sampati hak </p>
                        </div>
                        <div  class="mb-5">
                            <label for="formGroupExampleInput" class="form-label">Sampati hak</label>
                            <select class="form-select inputName" id="formGroupExampleInput" onchange="fun2(this.value)" aria-label="Default select example">
                                <option value=""  hidden selected>choose</option>
                                <option value="Yes">Yes</option>
                                <option value="No">No</option>
                            </select>
    
                        </div>
    
                        <div id="SSampati" class="row">
                            <div class="col-sm-6">
                                <div class="mb-5">
                                    <label for="formGroupExampleInput" class="form-label">के आदेश क्रमांक </label>
                                    <input type="text"  class="form-control inputName" id="formGroupExampleInput"   placeholder="ENTER SUMMARY">
                                </div>
                                <div  class="mb-5">
                                    <label for="formGroupExampleInput" class="form-label">कार्यालय </label>
                                    <input type="text"  class="form-control inputName" id="formGroupExampleInput"   placeholder="ENTER SUMMARY">
    
                                </div>
    
                            </div>
                            <div class="col-sm-6">
                               
                                <div class="mb-5">
                                    <label for="formGroupExampleInput" class="form-label">दिनांक </label>
                                    <input type="date" id="formGroupExampleInput" class="form-control inputName"   >
                                </div>
    
                                <div class="mb-5">
                                    <label for="formGroupExampleInput" class="form-label">द्वारा अनुमति आदेश जारी </label>
                                    <input type="file" id="formGroupExampleInput" class="form-control inputName"   >
                                </div>
    
                            </div>
                        </div>
                      
                            <!-- Aadiwasi ki bhumi ke sambandh me  -->
    
                            <div>
                                <p>Aadiwasi ki bhumi ke sambandh me </p>
                            </div>
                            <div  class="mb-5">
                                <label for="formGroupExampleInput" class="form-label">Aadiwasi ki bhumi ke sambandh me</label>
                                <select class="form-select inputName" id="formGroupExampleInput" onchange="fun3(this.value)" aria-label="Default select example">
                                    <option value=""  hidden selected>choose</option>
                                    <option value="Yes">Yes</option>
                                    <option value="No">No</option>
                                </select>
        
                            </div>
        
                            <div id="AAadiwasi" class="row">
                                <div class="col-sm-6">
                                    <div class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">के आदेश क्रमांक </label>
                                        <input type="text"required class="form-control inputName" id="formGroupExampleInput"   placeholder="ENTER SUMMARY">
                                    </div>
                                    <div  class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">कार्यालय </label>
                                        <input type="text" class="form-control inputName" id="formGroupExampleInput"   placeholder="ENTER SUMMARY">
        
                                    </div>
        
                                </div>
                                <div class="col-sm-6">
                                   
                                    <div class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">दिनांक </label>
                                        <input type="date" id="formGroupExampleInput" class="form-control inputName"   >
                                    </div>
        
                                    <div class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">द्वारा अनुमति आदेश जारी </label>
                                        <input type="file" id="formGroupExampleInput" class="form-control inputName"   >
                                    </div>
        
                                </div>
                            </div>
    
    
                            <!-- PRATIFAL -->
    
                            <div>
                                <p>PRATIFAL</p>
                            </div>
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">PRATIFAL KA PRAKAR </label>
                                        <select class="form-select inputName" id="formGroupExampleInput" aria-label="Default select example">
                                            <option value="Cash">Cash</option>
                                            <option value="Cheque">Cheque</option>
                                            <option value="RTGS">RTGS</option>
                                        </select>
                                    </div>
                                    <div  class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">JAARI DINANK </label>
                                        <input type="date" class="form-control inputName" id="formGroupExampleInput">
        
                                    </div>
        
                                </div>
                                <div class="col-sm-6">
                                   
                                    <div class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">CHECK/RTGS NO </label>
                                        <input type="number" id="formGroupExampleInput" class="form-control inputName"   >
                                    </div>
        
                                    <div class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">RASHI</label>
                                        <input type="number" id="formGroupExampleInput" class="form-control inputName"   >
                                    </div>
        
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">BANK KA NAAM </label>
                                        <input type="text" id="formGroupExampleInput" class="form-control inputName"   >
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="mb-5">
                                        <label for="formGroupExampleInput" class="form-label">SAMPATI VIVRAN KI AVASHAKTA </label>
                                        <select class="form-select inputName" id="formGroupExampleInput" aria-label="Default select example">
                                            <option value="One">One</option>
                                            <option value="Two">Two</option>
                                            <option value="Three">Three</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                   
                        <div class="submit">
                                <button type="submit" class="m-5 submitbtn">Save and next</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </section>
    <script>
        var d;
        function fun(arr,val){
            d=val;
            let doc=document.getElementById("docdes");
            let sub=document.getElementById("sub");
            let subdes=document.getElementById("subdes");
            let parsk=document.getElementById("parsk");
            let text=`<option  value="दस्तावेज का प्रकार"  hidden selected>दस्तावेज का प्रकार</option> `
            for(let i=0;i<arr.length;i++){
                if(arr[i].name==val){
                    doc.value=arr[i].description
                        for(let k=0;k<arr[i].sub.length;k++){
                            text=text+`<option  value="${arr[i].sub[k].name}">${arr[i].sub[k].name}</option> `
                        }
                    sub.innerHTML=text;
                    subdes.value=""
                    if(arr[i].par=="true"){
                        parsk.style.display="block";
                    }
                    else{
                        parsk.style.display="none";
                    }
                }
            }
        }

        function fun1(arr,val){
            console.log(d)
            let doc=document.getElementById("doc");
            let sub=document.getElementById("sub");
            let subdes=document.getElementById("subdes");
            for(let i=0;i<arr.length;i++){
                if(arr[i].name==d){
                        for(let k=0;k<arr[i].sub.length;k++){
                            if(arr[i].sub[k].name==val){
                                subdes.value=arr[i].sub[k].description
                            }
                        }
                }
            }
        }

        function fun2(value){
            let doc=document.getElementById("SSampati");
            if(value=="Yes"){
                doc.style.display="flex";
            }else{
                doc.style.display="none";

            }
        }

        function fun3(value){
            let doc=document.getElementById("AAadiwasi");
            if(value=="Yes"){
                doc.style.display="flex";
            }else{
                doc.style.display="none";

            }
        }
    </script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm"
        crossorigin="anonymous"></script>
